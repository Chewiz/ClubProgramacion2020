public class contrasena
{
    //Metodo de prueba
    public static void main (String []args){
        String xdefecto = "abcd1";
        
        verificacion(args[0], xdefecto);
        
    }
    
    //Metodo que verifica la contrase침a
    public static void verificacion( String cont, String xdefecto){
         System.out.println(xdefecto.equals(cont) ? "si jal칩" : "No jal칩");  //Embeded if
    }
}
